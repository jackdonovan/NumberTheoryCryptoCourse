    "def fermat_factor_v2(N):\n",
    "    t0 = ceil(sqrt(N))\n",
    "    k = 0\n",
    "    while True:\n",
    "        if is_square((t0+k)^2 - N) == True:\n",
    "            return t0 + k +sqrt((t0 + k)^2 - N), t0 +k - sqrt((t0+k)^2 - N), k\n",
    "        k+=1\n",
    "fermat_factor_v2(98587)"
   ]
